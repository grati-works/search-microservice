{
  "main": "./src/shared/infra/http/server.ts",
  "scripts": {
    "install": "docker pull valeriansaliou/sonic:v1.3.2",
    "start:sonic": "docker run -p 1491:1491 -v /path/to/search-microservice/src/sonic/config.cfg:/etc/sonic.cfg -v /path/to/search-microservice/src/sonic/store/:/var/lib/sonic/store/ -d valeriansaliou/sonic:v1.3.2",
    "start:server": "ts-node .",
    "start:server:dev": "ts-node-dev .",
    "start": "yarn start:sonic && yarn start:server",
    "start:dev": "yarn start:sonic && yarn start:server:dev"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "sonic-channel": "^1.2.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.23",
    "ts-node-dev": "^1.1.8"
  }
}
