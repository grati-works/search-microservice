    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.23",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "ts-node-dev": "^1.1.8"
  }
}
